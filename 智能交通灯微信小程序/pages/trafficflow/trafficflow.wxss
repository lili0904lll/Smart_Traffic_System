/* pages/trafficflow/trafficflow.wxss */
.container {
    padding: 20px;
  }
  
  .header {
    font-size: 18px;
    font-weight: bold;
    margin-bottom: 20px;
  }
  
  .content {
    margin-top: 20px;
  }
  
  .table {
    width: 100%;
    border-collapse: collapse;
  }
  
  .row {
    display: flex;
    border-bottom: 1px solid #ccc;
  }
  
  .row.header {
    background-color: #f0f0f0;
  }
  
  .cell {
    flex: 1;
    padding: 10px;
    text-align: center;
  }    

  
